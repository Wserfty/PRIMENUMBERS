<html>

<script>
var nums = [];
prime();
function prime(){
  create();
  cheak(nums,0);
  sum(nums,0,0);
}
function create(){
  for (var counter = 1; counter <= 25; counter++) {
      var notPrime = false;
      for (var i = 2; i <= counter; i++) {
          if (counter%i===0 && i!==counter) {
              notPrime = true;
          }
      }
      if (notPrime === false) {
                  nums.push(counter);
                  console.log(counter);
      }
  }
  function cheak(nums,i){
var current = [1,2,3,5,7,11,13,17,19,23];
if (i<nums.length){
if (nums[i]==current[i])
{
cheak(nums,i+1)
}
else 
{
console.log("ochibka")
}
}
return nums;
}
  function sum(nums,a,f)
  {
  	if(a<nums.length)
    {
    	f=f+nums[a];
      a=a+1;
      console.log(f);
      sum(nums,a,f);
    }
    else
    {
    	return f;
    }
  }
  

</script>

</html>
