<html>

<script>
var nums = [];
function prime(){
  create();
}
function create(){
  for (var counter = 1; counter <= 25; counter++) {
      var notPrime = false;
      for (var i = 2; i <= counter; i++) {
          if (counter%i===0 && i!==counter) {
              notPrime = true;
          }
      }
      if (notPrime === false) {
                  nums.push(counter);
                  console.log(counter);
      }
  }
  var nums= [1,2,3,5,7,11,13,17,19,23];
  function sum(nums,a,f)
  {
  	if(a<nums.length)
    {
    	f=f+nums[a];
      a=a+1;
      console.log(f);
      sum(nums,a,f);
    }
    else
    {
    	return f;
    }
  }
  sum(nums,0,0);


</script>

</html>
